{
  "name": "rx4d",
  "version": "1.0.1",
  "author": "Adrian C. Miranda",
  "description": "RegExp 4 dummies",
  "main": "index.js",
  "unpkg": "index.js",
  "tonicExampleFilename": "./example/runkit.js",
  "homepage": "https://github.com/adriancmiranda/rx4d#readme",
  "license": "MIT",
  "scripts": {
    "prereport:coverage": "nyc report --reporter=text-lcov npm test > test/coverage.lcov",
    "report:coverage": "codecov -f test/coverage.lcov --token=$CODECOV_TOKEN",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "test": "cross-env NODE_ENV=test ava test/*.unit.js --color",
    "test:coverage": "nyc npm test -- --no-cache --verbose",
    "dev": "run-p watch:*",
    "watch:test": "npm test -- --no-cache --watch",
    "lint": "eslint --ext .js index.js",
    "lint:fix": "npm run lint --fix",
    "travis:test": "npm test",
    "precommit": "run-s lint test",
    "commitmsg": "validate-commit-msg",
    "prerelease": "npm test",
    "release": "standard-version"
  },
  "dependencies": {
    "object-chain": "^1.0.2"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "codecov": "3.0.1",
    "conventional-commit-types": "2.2.0",
    "cross-env": "5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "0.14.3",
    "npm-check-updates": "2.14.2",
    "npm-run-all": "4.1.3",
    "nyc": "11.7.1",
    "standard-version": "4.3.0",
    "uglify-es": "3.3.9",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adriancmiranda/rx4d.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/rx4d/issues"
  },
  "files": [
    "example",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "regexp"
  ]
}
